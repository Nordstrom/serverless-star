{
  "name": "serverless-star",
  "version": "0.0.1",
  "description": "A serverless.com framework based job distribution tool to distribute a job over multiple cloud functions according to job and cloud provider specific plugins",
  "main": "index.js",
  "bin": {
    "serverless-star": "./bin/serverless-star",
    "star": "./bin/serverless-star"
  },
  "scripts": {
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha -r aws-sdk --recursive -R min tests",
    "lint": "./node_modules/.bin/eslint . bin/serverless-artillery",
    "publish": "./node_modules/.bin/np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nordstrom/serverless-star.git"
  },
  "author": "Nordstrom, Inc.",
  "contributors": [
    "Charles Sullivan <sullux@gmail.com> (https://github.com/sullux)",
    "Erik Erikson <erik.erikson@gmail.com> (https://github.com/erikerikson)"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Nordstrom/serverless-star/issues"
  },
  "homepage": "https://github.com/Nordstrom/serverless-artillery#readme",
  "dependencies": {
    "aws-sdk": "^2.5.0",
    "get-stdin": "^5.0.1",
    "js-yaml": "^3.6.1",
    "semver": "^5.3.0",
    "shortid": "^2.2.6",
    "yargs": "^6.2.0"
  },
  "devDependencies": {
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^3.0.2",
    "np": "^2.16.0",
    "nyc": "^11.0.3"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "tests",
      "test.js",
      "coverage"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".jsx"
    ],
    "cache": true,
    "all": true
  }
}
